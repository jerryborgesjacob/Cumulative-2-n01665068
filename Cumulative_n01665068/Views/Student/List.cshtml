@model List<Cumulative_n01665068.Models.Student>
@{
    ViewBag.Title = "List";
}

<!---This View Displays the list of all Students. Any Student's name can be clicked to view their full details-->
<h1>Students</h1>

@foreach (var Student in Model)
{
    <div>
        <a href="/Student/Show/@Student.StudentID">@Student.StudentName</a>
    </div>
}


