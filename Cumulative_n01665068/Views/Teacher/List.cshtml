@model List<Cumulative_n01665068.Models.Teacher>
@{
    ViewBag.Title = "List";
}

<!---This View Displays the list of all teachers. Any teacher's name can be clicked to view their full details-->
<h1>Academic Faculty</h1>

@foreach (var teacher in Model)
{
    <div class="list">
        <a href="/Teacher/Show/@teacher.TeacherID">@teacher.TeacherName</a>
    </div>

}

<div class="delete">
    <a href="/Teacher/New"> Add a New Teacher </a>
</div>

